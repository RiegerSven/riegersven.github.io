@article{goetghebeur2020,
  title = {Formulating Causal Questions and Principled Statistical Answers},
  author = {Goetghebeur, Els and Le Cessie, Saskia and De Stavola, Bianca and Moodie, Erica Em and Waernbaum, Ingeborg and {“on behalf of” the topic group Causal Inference (TG7) of the STRATOS initiative}},
